msgstr "секунд"

# LuCI Status Page
msgid "Mini-MWAN Status"
msgstr "Статус Mini-MWAN"

msgid "Real-time monitoring of WAN interface status"
msgstr "Мониторинг состояния WAN интерфейсов в реальном времени"

msgid "No status information available. Make sure the Mini-MWAN service is running."
msgstr "Информация о статусе недоступна. Убедитесь, что служба Mini-MWAN запущена."

msgid "Check with: "
msgstr "Проверка с помощью: "

msgid "Service Information"
msgstr "Информация о службе"

msgid "Mode:"
msgstr "Режим:"

msgid "Check Interval:"
msgstr "Интервал проверки:"

msgid "Last Update:"
msgstr "Последнее обновление:"

msgid "Interface Status"
msgstr "Состояние интерфейса"

msgid "Interface"
msgstr "Интерфейс"

msgid "Status"
msgstr "Статус"

msgid "Since"
msgstr "С"

msgid "Latency"
msgstr "Задержка"

msgid "Traffic (RX/TX)"
msgstr "Трафик (RX/TX)"

msgid "Gateway"
msgstr "Шлюз"

msgid "Last Check"
msgstr "Последняя проверка"

msgid "Status updates automatically every 5 seconds"
msgstr "Статус обновляется автоматически каждые 5 секунд"

# Configuration: Global Settings
msgid "Enable"
msgstr "Включить"

msgid "Enable Mini Multi-WAN service. At least two WAN interfaces must be configured."
msgstr "Включить службу Mini Multi-WAN. Должны быть настроены как минимум два WAN интерфейса."

msgid "Mode"
msgstr "Режим"

msgid "Operating mode: failover (primary/backup) or multi-uplink (load balancing)"
msgstr "Режим работы: отказоустойчивость (основной/резервный) или несколько каналов (балансировка нагрузки)"

msgid "Failover (Primary/Backup)"
msgstr "Отказоустойчивость (Основной/Резервный)"

msgid "Multi-Uplink (Load Balancing)"
msgstr "Несколько каналов (Балансировка нагрузки)"

msgid "Check Interval"
msgstr "Интервал проверки"

msgid "How often to check WAN connectivity (seconds)"
msgstr "Как часто проверять соединение WAN (в секундах)"

# Configuration: WAN Interface
msgid "Enabled"
msgstr "Включен"

msgid "Device"
msgstr "Устройство"

msgid "Network interface device (e.g., wg0, wg1, eth1)"
msgstr "Сетевое устройство интерфейса (например, wg0, wg1, eth1)"

msgid "Metric"
msgstr "Метрика"

msgid "Route metric (lower = higher priority). Used in failover mode only."
msgstr "Метрика маршрута (меньше = выше приоритет). Используется только в режиме отказоустойчивости."

msgid "Weight"
msgstr "Вес"

msgid "Traffic distribution weight for multi-uplink mode. Used in multi-uplink mode only."
msgstr "Вес распределения трафика для режима нескольких каналов. Используется только в режиме нескольких каналов."

msgid "Ping Target"
msgstr "Цель ping"

msgid "IP address to ping for connectivity check"
msgstr "IP-адрес для проверки связи с помощью ping"

msgid "Ping Count"
msgstr "Количество ping-запросов"

msgid "Number of ping attempts"
msgstr "Количество попыток ping"

msgid "Timeout"
msgstr "Таймаут"

msgid "Ping timeout (seconds)"
msgstr "Таймаут ping (в секундах)"
